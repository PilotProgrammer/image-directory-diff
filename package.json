{
  "name": "image-directory-diff",
  "version": "1.0.0",
  "description": "An AWS Lambda function to parse a pdf and synthesize to text using AWS Polly service.",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "help": "npm run build && node dist/Program.js --help",
    "start": "npm run build && node dist/Program.js",
    "watch": "tsc -w",
    "test": "jest"
  },
  "license": "ISC",
  "jest": {
    "automock": false,
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": ".coverage",
    "testRegex": ".*\\test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "modeulResolution": "node",
          "module": "commonjs",
          "target": "es6"
        }
      }
    },
    "preset": "ts-jest",
    "testMatch": null
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-loader": "^4.0.2",
    "file-loader": "^6.2.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "webpack": "^5.11.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.0"
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "ffprobe": "^1.1.2",
    "ffprobe-static": "^3.0.0",
    "file-type": "^16.5.4",
    "image-size": "^0.9.3",
    "path": "^0.12.7",
    "typescript": "^4.1.3",
    "typescript-logging": "^1.0.0",
    "yargs": "^15.3.1"
  }
}
